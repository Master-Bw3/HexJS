{
  "schemaVersion": 1,
  "id": "hexjs",
  "version": "${modVersion}",
  "name": "HexJs",
  "description": "HexJs addon for Hex Casting",
  "authors": [
    "Master_Bw3"
  ],
  "contact": {
    "homepage": "https://master_bw3.github.io/hexjs",
    "sources": "https://github.com/Master_Bw3/hexjs"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "io.github.master_bw3.hexjs.fabric.FabricHexJs"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "io.github.master_bw3.hexjs.fabric.FabricHexJsClient"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "io.github.master_bw3.hexjs.fabric.FabricHexJsModMenu"
    }]
  },
  "mixins": [
    "hexjs-common.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting}",
    "cloth-config": ">=${versions.clothConfig}",
    "kubejs": ">=${versions.kubejs}"
  },
  "suggests": {
    "modmenu": ">=${versions.modMenu}"
  }
}
